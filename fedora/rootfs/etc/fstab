/dev/mapper/fedora-root /                       xfs     ro        0 0
LABEL=FedoraBoot0 /boot                   xfs     defaults        0 0
LABEL=FedoraEFI0          /boot/efi               vfat    umask=0077,shortname=winnt 0 2